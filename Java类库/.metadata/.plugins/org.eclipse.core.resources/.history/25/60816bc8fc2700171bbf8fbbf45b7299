package constantPool;

public class ConstantPoolTest {
	//常量池
	
	//1.存放类文件的信息：类名、包名、字段名称类型、方法名...
	//2.存放static final 字段
	//3.存放常量字符串
	//4.存放基本类型包装类的对象
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//在编译时期就可以确定的数据，会被放入到常量池中
		String s1 = "abc";
		String s2 = "abc";
		
		System.out.println(s1 == s2);//结果是true 因为s1和s2都指向同一内存地址 
		
		String s3 = "ab";
		String s4 = "a";
		//在编译时期就不能确定的数据，不会被放入到常量池中
		String s5 = "s4+b";
		
		System.out.println(s3 == s5);//结果是false
		
		///编译器会对代码进行优化
		String s6 = "ab";
		String s7 = "a"+"b";//优化成String s7 = "ab";
		System.out.println(s6 == s7);//结果是true
		
		string s8 = new string("ab");
		
	}

}
