  </servlet>
  <servlet-mapping>
  	<servlet-name>mainServlet</servlet-name>
  	<url-pattern>/main</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>mySessionServlet</servlet-name>
