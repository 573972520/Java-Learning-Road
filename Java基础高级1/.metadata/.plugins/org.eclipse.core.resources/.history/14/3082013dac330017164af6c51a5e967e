package 正则表达式;

import java.util.regex.Pattern;

public class RegexTest1
{

	public static void main(String[] args)
	{
		
		/*String regex = "abcd";//正则表达式要求字符串为abc
		String str = "abcd";*/

		String regex = ".";
		String str = "a";
		boolean match = Pattern.matches(regex, str);
		System.out.println(match);
	}

}
