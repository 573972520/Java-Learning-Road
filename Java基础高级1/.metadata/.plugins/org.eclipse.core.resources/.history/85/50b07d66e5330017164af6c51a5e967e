package 正则表达式;

import java.util.regex.Pattern;

public class RegexTest7_位置匹配
{

	public static void main(String[] args)
	{
		// TODO Auto-generated method stub

		String regex = "^abc$";//
		String str = "abc";
		boolean match = Pattern.matches(regex, str);
		System.out.println(match);//ture
	}

}
