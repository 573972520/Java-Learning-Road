	{
		// TODO Auto-generated method stub
		List<Integer> list = new ArrayList<Integer>();
		
		for(int i = 0;i <10000;i++)
		{
			list.add(i);
		}
		
		Thread thread1 = new Thread(new Runnable()
		{
			
			@Override
			public void run()
			{
			for(int i = 0;i< list.size();i++)
			{
				
			}
			}
		})
	}

}
