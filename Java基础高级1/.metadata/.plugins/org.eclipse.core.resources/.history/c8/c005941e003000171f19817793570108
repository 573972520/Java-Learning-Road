public class 为什么要有List1
{

	public static void main(String[] args)
	{
		String[] strs1 = new String[5];
		String[] strs2 = { "avc", "123", "aaaa" };
		strs2 = new String[88];
		//strs2指向了一个新的数组对象，旧的数组长度没改变
		int[] nums = { 3, 43, 54, 65, 75 };
		//getData(nums);
		MyArrayList list = getData2(nums);
		for (int i = 0; i < list. size(); i++)
		{
			System.out.println(list.get(i));
		}

	}

	private static MyArrayList getData2(int[] nums)
	{
		MyArrayList list = new MyArrayList();
		for (int i = 0; i < nums.length; i++)
		{
			if (nums[i] % 3 == 0)
			{
				list.add(nums[i]);
			}

		}
		return list;
	}

	private static int[] getData(int[] nums)
	{
		//int [] result =new int[nums.length];
		int count = 0;
		for (int i = 0; i < nums.length; i++)
		{
			if (nums[i] % 3 == 0)
			{
				count++;
			}

		}
		int[] result = new int[count];
		int counter = 0;
		for (int i = 0; i < result.length; i++)
		{
			if (nums[i] % 3 == 0)
			{
				result[counter] = nums[i];
				counter++;
				System.out.println(nums[i]);
			}

		}
		return result;

	}
}
